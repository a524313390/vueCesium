{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports":true,
    "baseUrl": "./",
    "paths": {
 
        "@/*": ["./src/*"],
    },
      "types": ["element-plus/global"]
    
    
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/plugins/PolylineFlowMaterialProperty.js"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
